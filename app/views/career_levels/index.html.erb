
<h1>Career Levels</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @career_levels.each do |career_level| %>
      <tr>
        <td><%= career_level.name %></td>
        <td><%= link_to 'Show', career_level %></td>
        <td><%= link_to 'Edit', edit_career_level_path(career_level) %></td>
        <td><%= link_to 'Destroy', career_level, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Career Level', new_career_level_path %>
